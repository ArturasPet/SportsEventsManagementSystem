@model IEnumerable<SportBook.Models.TournamentMember>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FkTeamNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkTournamentNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FkTeamNavigation.TeamId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkTournamentNavigation.TournamentId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TournamentMemberId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TournamentMemberId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TournamentMemberId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
